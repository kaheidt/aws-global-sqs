# AWS Global SQS Environment Variables
# Copy this file to .env and update the values

# AWS Regions
AWS_REGION_PRIMARY=us-east-1
AWS_REGION_SECONDARY=us-west-2

# SQS Settings
SQS_QUEUE_NAME=global-sqs-demo-queue

# Route53 Settings
ROUTE53_HOSTED_ZONE_ID=YOUR_HOSTED_ZONE_ID
ROUTE53_DNS_NAME=sqs-global.example.com
ROUTE53_PRIMARY_WEIGHT=90
ROUTE53_SECONDARY_WEIGHT=10

# Application Settings
APPLICATION_NAME=global-sqs-demo

# Global SQS Endpoint (defaults to https://ROUTE53_DNS_NAME if not set)
# GLOBAL_SQS_ENDPOINT=https://sqs-global.example.com